{
  "name": "eregulations-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for eRegulations API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "stdio": "tsc && node dist/index.js",
    "sse": "tsc && node dist/sse.js",
    "dev": "ts-node-esm src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "mcp-inspector": "npx @modelcontextprotocol/inspector -e EREGULATIONS_API_URL=https://api-tanzania.tradeportal.org -e DEBUG=true node dist/index.js",
    "test-client": "node dist/test-client.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.0.1",
    "@types/better-sqlite3": "^7.6.12",
    "axios": "^1.6.2",
    "better-sqlite3": "^11.9.0",
    "express": "^4.18.2",
    "uuid": "^11.1.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  }
}
